{
	"name": "syllable-counter",
	"displayName": "Syllable Counter",
	"description": "Syllable counter tree view",
	"version": "0.0.1",
	"publisher": "joellefkowitz",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:syllableCounter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "syllableCounter",
					"name": "Syllable Counter"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.45.0",
		"typescript": "^3.9.4",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"minimist": "^0.2.1",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"syllable": "^4.1.0"
	}
}
